<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".AtlasGrzybowActivity">
    <ImageView
            android:layout_width="192dp"
            android:layout_height="144dp" app:srcCompat="@drawable/kurka" android:id="@+id/imageView2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageView3" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/textView" app:layout_constraintHorizontal_bias="0.551"
            app:layout_constraintVertical_bias="1.0"
            android:visibility="visible" tools:visibility="visible" android:layout_marginTop="100dp"
            android:layout_marginBottom="10dp" android:onClick="changeKurkaActivity"/>
    <ImageView
            android:layout_width="175dp"
            android:layout_height="140dp" app:srcCompat="@drawable/borowik" android:id="@+id/imageView3"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/textView2" android:layout_marginEnd="16dp"
            app:layout_constraintVertical_bias="0.0" android:layout_marginTop="45dp"
            android:onClick="changeBorowikActivity"/>
    <ImageView
            android:layout_width="160dp"
            android:layout_height="142dp" app:srcCompat="@drawable/muchomor" android:id="@+id/imageView4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/imageView5" app:layout_constraintVertical_bias="0.565"
            app:layout_constraintHorizontal_bias="0.62" android:onClick="changeMuchomorActivity"/>
    <ImageView
            android:layout_width="178dp"
            android:layout_height="136dp" app:srcCompat="@drawable/pieczarka" android:id="@+id/imageView5"
            app:layout_constraintTop_toBottomOf="@+id/textView2" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="16dp"
            app:layout_constraintVertical_bias="0.194" android:onClick="changePieczarkaActivity"/>
    <TextView
            android:text="@string/Cantharellus_cibarius"
            android:layout_width="132dp"
            android:layout_height="21dp" android:id="@+id/textView"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toTopOf="@+id/imageView4"
            app:layout_constraintEnd_toStartOf="@+id/textView2" app:layout_constraintHorizontal_bias="0.387"
            android:layout_marginBottom="16dp" android:onClick="changeKurkaActivity"/>
    <TextView
            android:text="@string/Boletus_edulis"
            android:layout_width="95dp"
            android:layout_height="23dp" android:id="@+id/textView2"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.819" app:layout_constraintVertical_bias="0.336"
            android:onClick="changeBorowikActivity"/>
    <TextView
            android:text="@string/Amanita_muscaria"
            android:layout_width="126dp"
            android:layout_height="27dp" android:id="@+id/textView3"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView4" app:layout_constraintEnd_toStartOf="@+id/textView4"
            app:layout_constraintHorizontal_bias="0.551" app:layout_constraintVertical_bias="0.0"
            android:onClick="changeMuchomorActivity"/>
    <TextView
            android:text="@string/Agaricus_campestris"
            android:layout_width="159dp"
            android:layout_height="30dp" android:id="@+id/textView4"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView5" android:layout_marginEnd="28dp"
            app:layout_constraintVertical_bias="0.021" android:onClick="changePieczarkaActivity"/>
</androidx.constraintlayout.widget.ConstraintLayout>